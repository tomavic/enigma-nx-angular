{
  "hosting": {
    "public": "dist/apps",
    "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
    "rewrites": [
      {
        "source": "**",
        "destination": "/home/index.html"
      },
      {
        "source": "/blog/**",
        "destination": "/blog/index.html"
      }
    ]
  }
}
